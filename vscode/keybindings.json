// Place your key bindings in this file to overwrite the defaults
[
    // left tab editor
    {"key": "ctrl+h", "command": "workbench.action.previousEditor"},

    // right tab editor
    {"key": "ctrl+l", "command": "workbench.action.nextEditor"},

    // close current editor
    {"key": "ctrl+k j", "command": "workbench.action.closeActiveEditor"},

    // open new editor
    {"key": "ctrl+k k", "command": "workbench.action.files.newUntitledFile"}

]